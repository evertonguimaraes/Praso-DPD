<pattern name="Observer Pattern">
	<entity id="e1" name="Component" type="Abstraction" />
	<entity id="e2" name="Composite" type="Specialization" />
	<entity id="e3" name="Leaf" type="Specialization" />

	<rule rule-type="Dependency"  source="e2" value="Specialize" target="e1" />
	<rule rule-type="Dependency"  source="e2" value="Typed" target="e1" />
	<rule rule-type="Dependency"  source="e3" value="Specialize" target="e1" />

	<resolver resolver-type="SEPARATE" source="e1" />
</pattern>