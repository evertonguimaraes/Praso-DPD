<pattern name="Observer Pattern - Abstract">
	<entity id="e1" name="Observer" type="Absolute" value="java.util.Observer" />
	<entity id="e2" name="Concrete Observer" type="Class" hasDependency="Specialize" />
	<entity id="e3" name="Abstract Subject" type="Absolute" value="java.util.Observable" />
	<entity id="e4" name="Subject" type="Any" hasDependency="Typed,Use" />

	<rule rule-type="Dependency" source="e2" value="Extend" target="e1" />
	<rule rule-type="Dependency" source="e4" value="Extend" target="e3" />
	<rule rule-type="Dependency" source="e4" value="Typed" target="e1" />
	<rule rule-type="Dependency" source="e4" value="Use" target="e1" />

	<rule rule-type="AST_Analyze" source="e3" value="Aggregates" target="e1" />
	<rule rule-type="AST_Analyze" source="e3" value="Loops" target="e1" />

	<resolver resolver-type="SEPARATE" source="e1" />

</pattern>