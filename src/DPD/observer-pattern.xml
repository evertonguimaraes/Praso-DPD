<pattern name="Observer Pattern">
	<entities>
		<entity id="e1" name="Observer" type="abstraction" />
		<entity id="e2" name="Concrete Observer" type="realization" />
		<entity id="e3" name="Subject" type="Class" />
	</entities>
	<rules>
		<rule type="IsRealization" source="e1" target="e2" />
		<rule type="IsPluralEntity" target="e2" />
		<rule type="IsUsed" source="e3" target="e1" />
		<rule type="IsTyped" source="e3" target="e1" />
	</rules>
</pattern>